# Environment Variables Template
# Copy this file to .env and fill in your actual values

# Google Sheets API Configuration
GOOGLE_SHEETS_SPREADSHEET_ID=your_google_sheets_id_here
GOOGLE_SHEETS_RANGE=A1:Z1000

# Pancake API Configuration
PANCAKE_ACCESS_TOKEN=your_pancake_access_token_here

# POS Pancake API Configuration
POS_ACCESS_TOKEN=your_pos_access_token_here
POS_SHOP_ID=your_shop_id_here

# Application Configuration
PRODUCT_STATE_FILE=product_state.json
LOG_LEVEL=INFO

# Optional: Cron Job Settings
CRON_INTERVAL_SECONDS=30
MAX_WORKERS=3

# Optional: Monitoring
ENABLE_MONITORING=true
MONITOR_PORT=8000






